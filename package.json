{
  "name": "react-crutch-boilerplate",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/Zelofff/react-crutch-boilerplate.git",
  "author": "zeloff <zelofflab@yandex.ru>",
  "license": "MIT",
  "scripts": {
    "dev": "webpack-dev-server --open --config ./config/webpack.dev.js",
    "build": "webpack --config ./config/webpack.prod.js",
    "analyze": "webpack --config ./config/webpack.analyze.js",
    "test": "jest --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": [
    "last 2 versions",
    "not dead",
    "not < 2%",
    "not ie 11"
  ],
  "lint-staged": {
    "*.{js, jsx}": [
      "prettier --write",
      "eslint ./src --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@hot-loader/react-dom": "^16.10.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-hot-loader": "^4.12.15"
  },
  "devDependencies": {
    "@arkweid/lefthook": "^0.6.3",
    "@testing-library/jest-dom": "^4.2.0",
    "@testing-library/react": "^9.3.0",
    "@types/jest": "^24.0.20",
    "@types/react": "^16.9.11",
    "@types/react-axe": "^3.1.0",
    "@types/react-dom": "^16.9.3",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "^3.0.0",
    "eslint": "^6.6.0",
    "eslint-plugin-jest": "^23.0.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.2.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "lint-staged": "^9.4.2",
    "prettier": "^1.18.2",
    "react-axe": "^3.3.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.6.4",
    "webpack": "^4.41.2",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.2.2"
  }
}